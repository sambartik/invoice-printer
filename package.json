{
  "name": "invoice-printer",
  "version": "1.0.0",
  "description": "Print invoices as you get them by email.",
  "main": "build/index.js",
  "scripts": {
    "test": "jest -c jest.config.js",
    "lint": "eslint --ext .js,.ts src",
    "clean": "rimraf build logs temp",
    "prebuild": "npm run clean",
    "build": "tsc -p tsconfig.json",
    "start:prod": "echo 'Starting in production environment...' && pm2 start ecosystem.config.js --env production",
    "start:dev": "echo 'Starting in development environment...' && pm2 start ecosystem.config.js --env development",
    "start": "npm run start:prod",
    "stop": "echo 'Stopping...' && pm2 stop ecosystem.config.js"
  },
  "keywords": [],
