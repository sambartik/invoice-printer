    "@typescript-eslint/parser": "^5.0.0",
    "chance": "^1.1.8",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^27.5.1",
    "prettier": "^1.19.1",
