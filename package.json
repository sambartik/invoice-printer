    "dotenv": "^16.0.0",
    "fs-extra": "^10.0.1",
    "imap-simple": "^5.1.0",
    "libmime": "^5.0.0",
    "pdf-parse": "^1.1.1",
    "pm2": "^5.2.0",
    "pushover-notifications": "^1.2.2",
    "unix-print": "^1.1.0",
    "uuid": "^8.3.2",
    "winston": "^3.6.0",
    "winston-daily-rotate-file": "^4.6.1"
  },
  "devDependencies": {
    "@logtail/types": "^0.1.8",
    "@types/chance": "^1.1.3",
